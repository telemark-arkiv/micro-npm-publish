{
  "name": "micro-npm-publish",
  "description": "publish to npm when there is a release on GitHub",
  "version": "1.0.20",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.10.0"
  },
  "scripts": {
    "build": "node lib/generate-npmrc.js",
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm -E tfk.env && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "github",
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-npm-publish.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-npm-publish/issues"
  },
  "homepage": "https://github.com/telemark/micro-npm-publish",
  "now": {
    "alias": [
      "npm.publish.micro.tjeneste.win"
    ],
    "type": "npm"
  },
  "devDependencies": {
    "ava": "0.23.0",
    "coveralls": "3.0.0",
    "nsp": "3.1.0",
    "nyc": "11.3.0",
    "standard": "10.0.3",
    "test-listen": "1.0.2"
  },
  "dependencies": {
    "axios": "0.17.1",
    "marked": "0.3.6",
    "micro": "9.0.1"
  }
}